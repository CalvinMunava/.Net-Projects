@model IEnumerable<Iserv.Models.ServiceOrderLine>

@{
    ViewBag.Title = "SOLreport";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div id="ConsRP">
    <h2>SOLreport</h2>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ServiceQuoteLine_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Service.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceOrder.ServiceOrder_Status.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceQuote.Total)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ServiceQuoteLine_ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Service.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ServiceOrder.ServiceOrder_Status.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ServiceQuote.Total)
                </td>
            </tr>
        }

    </table>
</div>