@model IEnumerable<Iserv.Models.ServiceOrder>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutCustomer.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ServiceOrder_Status.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServiceQuote.Item)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ServiceOrder_Status.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ServiceQuote.Item)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ServiceOrderID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ServiceOrderID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ServiceOrderID })
        </td>
    </tr>
}

</table>
