@model Iserv.Models.Province

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2 style="color:white">Province</h2>
<hr />

<div class="row" style="height:100%;">


    <div class="card " style="width:320px;height:auto;border:1px solid;border-radius:10px;padding:10px;background-color:rgba(0,0,0,0.88);margin-left:5px;margin-top:10px">
        <div class="card-body" >
            <h4 class="card-title" style="color:white"> Province: @Model.Name    </h4>
           

            <div class="btn-group">
                <button class="btn btn-info btn-sm" type="submit" onclick="window.location.href='@Url.Action("Edit", "Provinces", new { id = Model.ProvinceID })'">Modify</button>
                <button class="btn btn-dark btn-sm" type="submit" onclick="window.location.href='@Url.Action("Index", "Provinces")'">Back</button>
            </div>

        </div>
        <br />
    </div>

</div>